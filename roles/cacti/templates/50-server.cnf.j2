[mysqld]
user                    = mysql
pid-file                = /run/mysqld/mysqld.pid
socket                  = /run/mysqld/mysqld.sock
port                   = 3306
basedir                 = /usr
datadir                 = /var/lib/mysql
tmpdir                  = /tmp
lc-messages-dir         = /usr/share/mysql
bind-address            = 127.0.0.1
query_cache_size        = 16M
log_error = /var/log/mysql/error.log
expire_logs_days        = 10
collation-server = {{ mysql.collation_server }}
character-set-server  = {{ mysql.character_set_server }}
max_heap_table_size = {{ mysql.max_heap_table_size }}
tmp_table_size = {{ mysql.tmp_table_size }}
join_buffer_size = {{ mysql.join_buffer_size }}
innodb_file_format = {{ mysql.innodb_file_format }}
innodb_large_prefix = {{ mysql.innodb_large_prefix }}
innodb_buffer_pool_size = {{ mysql.innodb_buffer_pool_size }}
innodb_buffer_pool_instances = {{ mysql.innodb_buffer_pool_instances }}
innodb_flush_log_at_timeout = {{ mysql.innodb_flush_log_at_timeout }}
innodb_read_io_threads = {{ mysql.innodb_read_io_threads }}
innodb_write_io_threads = {{ mysql.innodb_write_io_threads }}
innodb_io_capacity = {{ mysql.innodb_io_capacity }}
innodb_io_capacity_max = {{ mysql.innodb_io_capacity_max }}

